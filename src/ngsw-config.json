{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js",
                    "/manifest.webmanifest"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**",
                    "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "petradw",
            "urls": [
                "http://equipenet.no-ip.info:8081/metodos/",
                "https://equipenet.no-ip.info:8081/metodos/",
                "http://localhost:8081/metodos/"
            ],
            "cacheConfig": {
                "strategy": "performance",
                "maxSize": 10000,
                "maxAge": "10h",
                "timeout": "5s"
            }
        }
    ]
}